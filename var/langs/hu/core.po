msgid "Sorry, but nothing matched your search criteria. Please try again with some different parameter combination."
msgstr "Sajnáljuk, de a keresés nem hozott eredményt. Próbáljon egy eltérő kombinációt, vagy keresőszót!"

msgctxt "Languages::text_not_allowed_to_upload_file_extension"
msgid "It is not allowed to upload files with the <b>.[ext]</b> extension here."
msgstr "A <b>.[ext]</b> kiterjesztésű fájlok feltöltése itt nem engedélyezett."

msgctxt "Languages::text_not_valid_email"
msgid "<b>[email]</b> is not a valid e-mail address."
msgstr "A(z) <b>[email]</b> nem érvényes e-mail cím."

msgctxt "Languages::text_no_active_promotions"
msgid "No promotions available."
msgstr "Nincs elérhető promóció."

msgctxt "Languages::text_no_conflicts"
