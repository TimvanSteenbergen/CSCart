<?xml version="1.0" encoding="UTF-8"?>
<addon scheme="2.0">
    <id>twigmo</id>
    <name>Twigmo</name>
    <description>Adds the mobile admin support</description>
    <priority>100500</priority>
    <position>0</position>
    <status>active</status>
    <auto_install>MULTIVENDOR,ULTIMATE</auto_install>
    <default_language>en</default_language>
    <settings layout="separate" edition_type="ROOT,ULT:VENDOR">
        <sections>
            <section id="service" edition_type="ROOT">
                <name>Twigmo account</name>
                <items>
                    <item id="connect">
                        <type>template</type>
                        <default_value>connect.tpl</default_value>
                    </item>
                </items>
            </section>
            <section id="storefront" edition_type="ROOT,ULT:VENDOR">
                <name>Mobile Storefront</name>
                <items>
                    <item id="settings">
                        <type>template</type>
                        <default_value>settings.tpl</default_value>
                    </item>
                </items>
            </section>
            <section id="admin_app" edition_type="ROOT">
                <name>Admin app</name>
                <items>
                    <item id="admin_app">
                        <type>template</type>
                        <default_value>admin_app.tpl</default_value>
                    </item>
                </items>
            </section>
            <section id="addon" edition_type="ROOT">
                <name>Upgrade</name>
                <items>
                    <item id="upgrade">
                        <type>template</type>
                        <default_value>upgrade.tpl</default_value>
                    </item>
                </items>
            </section>
        </sections>
    </settings>
