msgctxt "Addons::name::access_restrictions"
msgid "Access restrictions"
msgstr "Toegangsbeperkingen"

msgctxt "Addons::description::access_restrictions"
msgid "Lets you limit storefront and administrator area access to certain IP-addresses with different options"
msgstr "Beperk de toegang tot uw webwinkel en beheerderspagina's tot bepaalde IP-adressen met verschillende opties"

msgctxt "Languages::access_restrictions"
msgid "Access restrictions"
msgstr "Toegangsbeperkingen"

msgctxt "Languages::add_ips"
msgid "Add IPs"
msgstr "IP-adressen toevoegen"

msgctxt "Languages::new_ips"
msgid "New IPs"
msgstr "Nieuwe IP-adressen"

msgctxt "Languages::add_domains"
msgid "Add domains"
msgstr "Domeinen toevoegen"

msgctxt "Languages::new_domains"
msgid "New domains"
msgstr "Nieuwe domeinen"

msgctxt "Languages::add_emails"
msgid "Add e-mail addresses"
msgstr "E-mailadressen toevoegen"

msgctxt "Languages::new_emails"
msgid "New e-mail addresses"
msgstr "Nieuwe e-mailadressen"

msgctxt "Languages::new_credit_card"
msgid "New credit card"
msgstr "Nieuwe creditcard"

msgctxt "Languages::add_credit_card"
msgid "Add credit card"
msgstr "Creditcard toevoegen"

msgctxt "Languages::privileges.manage_access_restrictions"
msgid "Manage access restrictions"
msgstr "Toegangsbeperkingen beheren"

msgctxt "Languages::domain"
msgid "Domain"
msgstr "Domein"

msgctxt "Languages::warning_of_ip_adding"
msgid "The entered IP address <b>[entered_ip]</b> forbids access to the administration panel from your computer (your IP is <b>[your_ip]</b>)."
msgstr "Het ingevoerde IP-adres <b>[entered_ip]</b> verbiedt toegang tot het Beheerderspaneel van uw computer (uw IP is <b>[your_ip]</b>)."

msgctxt "Languages::text_cc_number_is_blocked"
msgid "The credit card number <b>[cc_number]</b> is blocked by the administrator. Please contact the store administrator or try another one."
msgstr "Het creditcard nummer <b>[cc_number]</b> wordt geblokkeerd door de beheerder. Neem contact op met de beheerder of probeer een andere kaart."

msgctxt "Languages::text_not_valid_ip"
msgid "<b>[ip]</b> is not a valid ip address."
msgstr "<b>[ip]</b> is geen geldig IP-adres."

msgctxt "Languages::text_not_valid_domain"
msgid "<b>[domain]</b> is not a valid domain name."
msgstr "<b>[domain]</b> is geen geldige domeinnaam."

msgctxt "Languages::text_not_valid_cc_number"
msgid "<b>[cc_number]</b> is not a valid credit card number."
msgstr "<b>[cc_number]</b> is geen geldig creditcard nummer."

msgctxt "Languages::text_email_is_blocked"
msgid "The e-mail address <b>[email]</b> is blocked by the administrator. Please contact the store administrator or try another one."
msgstr "Het e-mailadres <b>[email]</b> is geblokkeerd door de beheerder. Neem contact op met de beheerder of probeer een ander e-mailadres."

msgctxt "Languages::text_ips_denied"
msgid "You are not allowed to access this area"
msgstr "U heeft geen toegang tot deze pagina"

msgctxt "Languages::text_ip_blocked_failed_login"
msgid "The IP was blocked after [number] unsuccessful logins"
msgstr "Het IP is geblokkeerd na [number] mislukte aanmeldingen"

msgctxt "Languages::text_ip_is_blocked"
msgid "Your IP address is blocked."
msgstr "Uw IP-adres is geblokkeerd."

msgctxt "Languages::store_admin"
msgid "Store administrator"
msgstr "Beheerder"

msgctxt "Languages::your_ip_added"
msgid "Your IP address <b>[ip]</b> has been added to the access restrictions table."
msgstr "Uw IP-adres <b>[ip]</b> is toegevoegd aan de tabel met toegangsbeperkingen."

msgctxt "Languages::your_ip_enabled"
msgid "Your IP address <b>[ip]</b> has been enabled in the access restrictions table."
msgstr "Uw IP-adres <b>[ip]</b> is ingeschakeld in de tabel met toegangsbeperkingen."

msgctxt "Languages::your_ip_removed"
msgid "Your IP address <b>[ip]</b> has been removed from the access restrictions table."
msgstr "Uw IP-adres <b>[ip]</b> is verwijderd uit de tabel met toegangsbeperkingen."

msgctxt "Languages::ip_from"
msgid "IP from"
msgstr "IP-adres van"

msgctxt "Languages::ip_to"
msgid "IP to"
msgstr "IP-adres naar"

msgctxt "Languages::text_access_notice"
msgid "You can use the wildcards for the \"Domain\", \"E-mail\" and \"Credit Card\" tabs: <br /><b>*</b> - can represent any number of characters, including zero<br /><b>?</b> - can represent any single character."
msgstr "U kunt de wildcards gebruiken voor de tabbladen \"Domein\", \"E-mail\" en \"Creditcard\": < br / > <b>*</b> - kan een willekeurig aantal tekens weergeven, inclusief nul < br / > <b>?</b> - kan een willekeurig teken weergeven."

