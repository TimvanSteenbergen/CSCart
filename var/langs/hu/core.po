msgstr "A <strong>[directory]</strong> mappa törlése sikertelen. Kérjük ellenőrizze a jogosultságokat."

msgctxt "Languages::text_cannot_delete_file"
msgid "Cannot delete the file <b>[file]</b>. Please check the permissions."
msgstr "A <strong>[file]</strong> fájl törlése sikertelen. Kérjük ellenőrizze a jogosultságokat."

msgctxt "Languages::text_cannot_rename_directory"
