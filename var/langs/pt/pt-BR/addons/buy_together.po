msgstr "[product_name] não pode ser adicionado a combinação. Compre junto não é compatível com o addon de cobrança recorrente"

msgctxt "Languages::buy_together_combination_cannot_be_added"
msgid "Combination cannot be added to cart"
msgstr "Combinação não pode ser adicionada ao carrinho"

msgctxt "Languages::buy_together_product_was_removed"
msgid "[product] did not have enough amount ([amount]) and was removed from the cart"
msgstr "[product] não tem estoque suficiente ([amount]) e foi removido do carrinho"

msgctxt "Languages::buy_together_fill_the_mandatory_fields"
