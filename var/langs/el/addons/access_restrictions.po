msgctxt "Languages::add_domains"
msgid "Add domains"
msgstr "Προσθήκη νέου ονόματος χώρου"

msgctxt "Languages::new_domains"
msgid "New domains"
msgstr "Νέα Domain"

msgctxt "Languages::add_emails"
msgid "Add e-mail addresses"
msgstr "Προσθήκη Διευθύνσεων e-mail"

