msgctxt "Addons::name::access_restrictions"
msgid "Access restrictions"
msgstr "Restrições de acesso"

msgctxt "Addons::description::access_restrictions"
msgid "Lets you limit storefront and administrator area access to certain IP-addresses with different options"
msgstr "Permite limitar o acesso da loja e administrador para determinados endereços IP com opções diferentes"

msgctxt "Languages::access_restrictions"
msgid "Access restrictions"
msgstr "Restrições de acesso"

msgctxt "Languages::add_ips"
msgid "Add IPs"
msgstr "Adicionar IPs"

msgctxt "Languages::new_ips"
msgid "New IPs"
msgstr "Novos IPs"

